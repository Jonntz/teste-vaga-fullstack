generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Costumer {
  id          Int      @id @default(autoincrement())
  nrInst      String
  nrAgencia   String
  cdClient    String
  nmClient    String
  nrCpfCnpj   String
  nrContrato  String
  dtContrato  String
  qtPrestacoes String
  vlTotal     String
  cdProduto   String
  dsProduto   String
  cdCarteira  String
  dsCarteira  String
  nrProposta  String
  nrPresta    String
  tpPresta    String
  nrSeqPre    String
  dtVctPre    String
  vlPresta    String
  vlMora      String
  vlMulta     String
  vlOutAcr    String	
  vlIof       String
  vlDescon    String
  vlAtual     String
  idSituac    String
  idSitVen    String
  status      String
}